<ivysettings>
	<settings	
			defaultResolver="my-repository"
			defaultConflictManager="all" />		<!-- in order to get all revisions without any eviction -->
	<resolvers>
  		<filesystem name="my-repository">
  			<artifact pattern="I:\hudsonbuilds\jobs\Validation\workspace\Libraries\[artifact]-[revision].[ext]"/>
  			<artifact pattern="I:\hudsonbuilds\jobs\Validation\workspace\Libraries\[artifact].[ext]"/> 
			<artifact pattern="C:\Apache\CXF\apache-cxf-2.1\lib\[artifact]-[revision].[ext]"/>
			<!--artifact pattern="${APACHE_CXF_LIB_PATH}\[artifact]-[revision].[ext]"/-->
			<artifact pattern="C:\Apache\CXF\apache-cxf-2.1\lib\[artifact].[ext]"/>
			<!--artifact pattern="${APACHE_CXF_LIB_PATH}\[artifact].[ext]"/-->
			<ivy pattern="I:\hudsonbuilds\jobs\Validation\workspace\[module]\ivy.xml"/>
			<artifact pattern="C:\Eclipse Projects\Libraries\[artifact]-[revision].[ext]"/>
			<!--artifact pattern="${EXTERNAL_DEPS}\[artifact]-[revision].[ext]"/-->
  			<artifact pattern="C:\Eclipse Projects\Libraries\[artifact].[ext]"/> 
  			<!--artifact pattern="${EXTERNAL_DEPS}\[artifact].[ext]"/-->
			<artifact pattern="C:\Apache\CXF\apache-cxf-2.1\lib\[artifact]-[revision].[ext]"/>
			<!--artifact pattern="${APACHE_CXF_LIB_PATH}\[artifact]-[revision].[ext]"/-->
			<artifact pattern="C:\Apache\CXF\apache-cxf-2.1\lib\[artifact].[ext]"/>
			<!--artifact pattern="${APACHE_CXF_LIB_PATH}\[artifact].[ext]"/-->
			<ivy pattern="C:\Eclipse Projects\[module]\ivy.xml"/>
			<!--ivy pattern="${ECLIPSE_PROJECT_PATH}\[module]\ivy.xml"/-->
			<!-- A repository for jenkins build-->
			<artifact pattern="C:\jenkinsbuilds\jobs\Validation Main Branch\workspace\Libraries\[artifact]-[revision].[ext]"/>
  			<artifact pattern="C:\jenkinsbuilds\jobs\Validation Main Branch\workspace\Libraries\[artifact].[ext]"/>
  			<ivy pattern="C:\jenkinsbuilds\jobs\Validation Main Branch\workspace\[module]\ivy.xml"/> 
  		</filesystem>
	</resolvers>
</ivysettings>